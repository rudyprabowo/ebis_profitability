[app-config]
app_alias = "MYAPP"
use_laminas_loader = 0
config_cache_enabled = 0
module_map_cache_enabled = 0
cache_dir = "data/cache/"

[layout]
default = "blank"

[cache]
session.ttl = 0
view.ttl = 1800
data.ttl = 1800

[db-mysql]
;user with select,insert,update delete permission on sys and app db
admin.driver = "Pdo_Mysql"
admin.hostname = "localhost"
admin.port = 3306
admin.username = ""
admin.password = ""
admin.database = ""

;user with select permission only on sys db
sys.driver = "Pdo_Mysql"
sys.hostname = "localhost"
sys.port = 3306
sys.username = ""
sys.password = ""
sys.database = ""

;user with select permission only on app db
app.driver = "Pdo_Mysql"
app.hostname = "localhost"
app.port = 3306
app.username = ""
app.password = ""
app.database = ""

